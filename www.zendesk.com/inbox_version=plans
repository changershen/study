<script type="text/javascript">
    (function() {
      /**
       * Custom login drop down logic for Inbox. Inbox subdomains are prefixed
       * and namespaced with "inbox_subdomain".
       * E.g. "inbox_subdomain:foobar" => "foobar@zendesk.com/inbox/"
       */
      var origin, prefix, hideLoginDropdown;

      origin = 'https://www.zndsk.com';
      prefix = 'inbox_subdomain';
      hideLoginDropdown = function() {
        $('.login-subdomain-suggestions').hide();
        $('.login-remind-me-subdomain').css({
          'float': 'none',
          'margin-left': 'auto',
          'margin-right': 'auto'
        });
      };

      // Webkit gets a scrollable list of subdomains
      if ($.browser.webkit) {
        $('ul.subdomainlogin ul').addClass('webkit');
      }

      var storage = new CrossStorageClient(origin +'/hub.html');

      storage.onConnect().then(function() {
        return storage.getKeys();
      }).then(function(keys) {
        var loginBtn, subdomains, ul;

        if (!keys || !keys.length) return hideLoginDropdown();

        loginBtn = $('ul.subdomainlogin .btn-login');

        // Need to filter out inbox keys
        subdomains = keys.filter(function(key) {
          return (key.indexOf(prefix) !== -1);
        }).map(function(key) {
          var sub = key.split(':')[1];
          return {
            name: sub + '.zendesk.com',
            url: 'https://' + sub + '.zendesk.com/inbox/'
          };
        });

        if (!subdomains.length) return hideLoginDropdown();

        // If there's only 1 subdomain, we update the login button url and return
        if (subdomains.length === 1) {
          return loginBtn.attr('href', subdomains[0].url);
        }

        // Otherwise we have multiple subdomains, so we first disable click
        // behaviour on the button itself
        loginBtn.click(function(e) {
          e.preventDefault();
          return false;
        });

        // Indicate that the user has multi subdomains for styling the button
        loginBtn.addClass('multi-subdomains');

        ul = $('ul.subdomainlogin ul');
        subdomains.forEach(function(subdomain) {
          var li = $('<li><a href="' + subdomain.url + '">'
            + subdomain.name + '</a></li>');
          ul.append(li);
        });
      })['catch'](function(err) {
        console.log(err);
        hideLoginDropdown();
      });
    }());

    // Banner slideshow
    function slideshow(target, fadeSpeed, duration) {
      timer = setInterval(function() {
        var curPhoto = '';

        $(target).each( function() {
          if($(this).css('z-index') == 2) {
            curPhoto = $(this);
          } else {
            $(this).css('z-index',0);
          }
        });

        if(curPhoto != null && curPhoto.length > 0 && typeof curPhoto.next() != 'undefined') {
          var nxtPhoto = curPhoto.next(target);

          if(nxtPhoto.length == 0)
            nxtPhoto = $(target+':first');

          nxtPhoto.css('z-index',1);
          curPhoto.animate(fadeSpeed,
            function() {
              curPhoto.css('z-index',1);
              nxtPhoto.css('z-index',2);
            }
          );
        }
      }, duration);
      return timer;
    }

    //Slide Form animation
    $(function(){
      $('.reg-account').click(function(){
        $('.form-contain').animate({ "top": "0px" }, 325 );
        setTimeout( function(){
          $('.play-button, .header-content h1').css("visibility", "hidden");
        },100);
        setTimeout( function(){
          $('.reg-account').css("visibility", "hidden");
        },245);
        setTimeout( function(){
          $('.create-account').css("visibility", "visible");
        },245);
        setTimeout( function(){
          $('.header-content p, .header-content a').css("visibility", "hidden");
        },240);
        setTimeout( function(){
          $('.create p').css("visibility", "visible");
        },250);
      });

      /*
       * Carousel init
       *
       */
       // adjust number of quotes
      $('.customer-testimonials li:first').css('z-index',5);
      slideshow('.customer-testimonials li', 1000, 6000);

      /*
       * Launch login modal
       *
       */

      $('.login-link').click(function(){
        $('#login-help').slideDown()
        $('.background-fade').show()
        $("html,body").css("overflow","hidden");
      });

      /*
       * Hide login modal
       *
       */

      $('.background-fade, .fade .module-close').click(function(){
        $('#login-help').slideUp()
        $('.background-fade').hide()
        $("html,body").css("overflow","auto");
      });

      $('.modal-content').on('click', function(e) {
        e.stopPropagation();
      });

      /*
       * Send reminder email
       *
       */

      $('.btn-remindme').on('click', function(){
        var email = $("#email").val()
          , url = 'https://support.zendesk.com/accounts/reminder?email=' + email + '&callback=?'
          , top = $('.recover h2').outerHeight()
          , height = $('.recover').innerHeight();

        if(email.length < 0 || email == 'me@example.com' || !/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(email )) {
          error
            .css('margin-left',20)
            .animate({'opacity': 1, 'margin-left':0}, 200);

        } else {
          $('form .loading')
            .css({'opacity':0, 'display':'block'})
            .animate({
              'opacity': 1, 'top':-Math.abs(top)-30,
              'height': height-70
            }, 200);

          $.getJSON(url, function(data) {
            var result = data.success
              , res = $('.remind-me-form .res');

            if(result == true) {
              res.show().removeClass('bad').html('<span class="good">Thanks! We\'re checking our records and if you have an account you should receive an email within a few minutes.</span>');
              $('img#loading').fadeOut('fast');
            } else {
              $('.loading').fadeOut('fast');
              res.show().addClass('bad').html('Whoops.  We can\'t find that email address. Did you spell it correctly?');
            }
          });
        }

        return false;
      });
  });
  </script>
